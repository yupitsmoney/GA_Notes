<h1 class="jumbotron">Please Log In</h1>

<h1>Log In</h1>

<%= form_for :login do |f| %>

<div class="form-group">
  <%= f.label :email %>
  <%= f.text_field :email, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.label :password %>
  <%= f.text_field :password, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.submit "Log In", class: "btn btn-primary" %>
</div>

<% end %>